{
	"name": "Kwality.Control.Http",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"customizations": {
		"vscode": {
			"settings": {
				"editor.formatOnSave": true,
				"files.exclude": {
					"app/Kwality.Control.Http/bin/": true,
					"app/Kwality.Control.Http/obj/": true,
					"app/Kwality.Control.Http.Tests/bin/": true,
					"app/Kwality.Control.Http.Tests/obj/": true
				}
			},
			"extensions": [
				"ms-dotnettools.csharp",
				"fernandoescolar.vscode-solution-explorer"
			]
		}
	},
	"remoteUser": "vscode"
}